     <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/users.js"></script>
</head>
<body class="bg">
    <!-- CONTAINER -->
    <br />
    <div class="container my-5 d-flex justify-content-center">
        <img src="https://images.unsplash.com/photo-1503023345310-bd7c1de61c7d?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1530&q=80" class="rounded-circle" width="80" height="80" />

    </div>
    <div>
        <div class="h3 mt-4 text-white text-center">John Doe</div>
    </div>
    <div class="container">
        <div class="row align-items-center justify-content-center">

            <div class="col-xs-12 col-sm-7 col-md-4 col-lg-7">
                <div class="b-form text-center">
                    <form>
                        <div class="form-group">
                            <input class="form-control rounded-5" id="UserName" type="text" name="username" placeholder="UserName">
                            <i class="fas fa-user b-font"></i>
                        </div>
                        <div class="form-group">
                            <input class="form-control rounded-5" id="Password"type="password" name="password" placeholder="Password">
                            <i class="fas fa-lock b-font"></i>
                        </div>
                        
                        <div class="form-group ">
                            <input class="form-control rounded-5" type="email" id="Email" name="email" placeholder="Email Address">
                            <i class="fas fa-envelope b-font"></i>
                        </div>
                        <button type="button" class="btn btn-dark rounded-5 btn-block" onclick="AddUser()">Sign Up</button>
                    </form>
                </div>

            </div>
        </div>
    </div>

    <div class="container my-4 d-flex justify-content-center">
        <p class="text-white">Already have a account ? </p>&nbsp;
        <a class="text-white" asp-area="" asp-controller="Home" asp-action="SignIn"> Sign In</a>
    </div>
    
</body>
</html>
